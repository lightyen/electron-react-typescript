{
	"extends": "../tsconfig.base",
	"compilerOptions": {
		"noImplicitAny": true,
		"noEmit": true,
		"jsx": "preserve",
		"module": "esnext",
		"moduleResolution": "node",
		"target": "esnext",
		"lib": ["esnext", "dom", "dom.iterable"],
		"types": ["react", "webpack-env"],
		"baseUrl": ".",
		"outDir": "",
		"paths": {
			"~/*": ["./*"],
			"~assets/*": ["../../assets/*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx", "**/*.js"]
}
