{
	"extends": "../../tsconfig",
	"compilerOptions": {
		"noImplicitAny": false,
		"suppressImplicitAnyIndexErrors": true,
		"skipLibCheck": true,
		"jsx": "preserve",
		"module": "esnext",
		"moduleResolution": "node",
		"target": "esnext",
		"lib": ["esnext", "dom", "dom.iterable"],
		"types": ["react", "webpack-env"],
		"baseUrl": ".",
		"paths": {
			"~/*": ["./*"],
			"~assets/*": ["../../assets/*"]
		}
	}
}
