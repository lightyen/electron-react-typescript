{
  "compilerOptions": {
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "allowJs": true,
    "noEmit": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "types": ["electron"],
    "lib": ["esnext", "dom", "webworker"],
    "baseUrl": "."
  }
}
