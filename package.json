  "license": "MIT",
  "keywords": [
    "electron",
    "react",
    "typescript"
  ],
  "scripts": {
    "build:renderer": "cross-env TS_NODE_PROJECT=\".webpack/tsconfig.json\" webpack --config .webpack/webpack.prod.ts",
    "build:main": "cross-env TS_NODE_PROJECT=\".webpack/tsconfig.json\" webpack --config .webpack/electron.prod.ts",
    "start:renderer": "cross-env TS_NODE_PROJECT=\".webpack/tsconfig.json\" webpack-dev-server --config .webpack/webpack.dev.ts",
    "start:main": "yarn && node debug/nodemon.js",
    "analyzer": "cross-env TS_NODE_PROJECT=\".webpack/tsconfig.json\" webpack --config .webpack/webpack.analyzer.ts",
    "doit": "yarn && yarn upgrade-interactive --latest",
    "test": "yarn && yarn run build:main && yarn run build:renderer",
    "build": "yarn && yarn run build:main && yarn run build:renderer && electron-builder --publish never",
    "release": "yarn && yarn run build:main && yarn run build:renderer && electron-builder build --win --publish always",
    "win-create-self-signed-cert": "electron-builder create-self-signed-cert"
  },
