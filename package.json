  "scripts": {
    "webpack-electron-prod": "webpack --config .webpack/electron.prod",
    "webpack-production": "webpack --config .webpack/webpack.prod",
    "start:renderer": "yarn && webpack-dev-server --config .webpack/webpack.dev",
    "start:main": "yarn && node debug/nodemon.js",
    "analyzer": "webpack --config .webpack/webpack.analyzer",
    "doit": "yarn && yarn upgrade-interactive --latest",
    "test": "yarn && yarn run webpack-electron-prod && yarn run webpack-production",
    "build": "yarn && yarn run webpack-electron-prod && yarn run webpack-production && electron-builder --publish never",
    "release": "yarn && yarn run webpack-electron-prod && yarn run webpack-production && electron-builder build --win --publish always",
    "win-create-self-signed-cert": "electron-builder create-self-signed-cert"
  },
