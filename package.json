{
  "name": "electron-react-typescript",
  "main": "main.js",
  "description": "use electron and react with typescript",
  "version": "1.5.0",
  "author": {
    "name": "lightyen <lightyen0123@gmail.com>"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lightyen/electron-react-typescript.git"
