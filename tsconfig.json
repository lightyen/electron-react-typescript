{
	"compilerOptions": {
		"strict": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"noImplicitThis": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"removeComments": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"sourceMap": true
	}
}
