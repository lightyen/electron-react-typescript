{
  "presets": [
    ["@babel/preset-react"],
    ["@babel/preset-env"],
    ["@emotion/babel-preset-css-prop"]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "emotion",
    "macros",
    [
      "react-intl",
      {
        "idInterpolationPattern": "[contenthash:5]",
        "extractFromFormatMessageCall": true,
        "ast": true
      }
    ]
  ],
  "env": {
    "development": {
      "plugins": ["react-refresh/babel"]
    }
  }
}
